    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "babel-loader": "^8.2.2",
    "dotenv": "^10.0.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
